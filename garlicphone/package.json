    "firebase": "8.4.3",
    "framer-motion": "4.1.6",
    "gif-encoder-2": "1.0.5",
    "husky": "6.0.0",
    "js-file-download": "0.4.12",
    "knuth-shuffle": "1.0.8",
    "lazy-brush": "1.0.1",
